        'red':                  {'r': 1.0,  'g': 0.0,   'b': 0.0,   'w': 0.0},
        'orange':               {'r': 1.0,  'g': 0.43,  'b': 0.0,   'w': 0.0},
        'green':                {'r': 0.0,  'g': 1.0,   'b': 0.0,   'w': 0.0},
